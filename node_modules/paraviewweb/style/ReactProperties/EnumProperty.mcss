.input {
    width: 100%;
    height: 28px;
    border-radius: 5px;
    background-color: white;
    border-width: 1px;
    border-color: lightgray lightgray gray lightgray;
    border-style: solid;
}

.inputMultiSelect {
  composes: input;
  height: 5.5em;
}

.header {
  composes: header from './CellProperty.mcss';
  margin-bottom: 6px;
}

.soloContainer {
  composes: container from './CellProperty.mcss';
  width: calc(100% - 4px);
  padding: 2px;
}
